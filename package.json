{
  "name": "@brandon7lewis/gis-survey.js",
  "version": "1.0.0",
  "description": "Mapping and surveying tools module for GIS and RTK surveying",
  "main": "dist/gis-survey.js",
  "module": "dist/gis-survey.esm.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publish": [
    {
      "path": "@semantic-release/npm",
      "npmPublish": true,
      "pkgRoot": "./",
      "access": "public"
    }
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/**/*.js'",
    "docs": "jsdoc -c jsdoc.config.json",
    "prepublishOnly": "npm run lint && npm run build",
    "serve": "node server.js"
  },
  "keywords": [
    "gis",
    "survey",
    "mapping",
    "geospatial",
    "3d",
    "rtk"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/BrandonLewis/gis-survey.js",
  "peerDependencies": {
    "gnss.js": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "gnss.js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/config-array": "^0.20.0",
    "@eslint/object-schema": "^2.1.6",
    "@rollup/plugin-terser": "^0.4.4",
    "better-docs": "^2.7.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.26.0",
    "glob": "^10.4.1",
    "jsdoc": "^4.0.4",
    "jsdoc-to-markdown": "^9.1.1",
    "lru-cache": "^10.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.40.2"
  },
  "type": "module"
}
