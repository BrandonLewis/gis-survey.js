{
  "name": "@brandon7lewis/gis-survey.js",
  "version": "1.0.38",
  "description": "Mapping and surveying tools module for GIS and RTK surveying",
  "main": "dist/gis-survey.js",
  "module": "dist/gis-survey.esm.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "echo \"No tests specified yet\" && exit 0",
    "lint": "eslint 'src/**/*.js'",
    "docs": "jsdoc -c jsdoc.config.json",
    "prepublishOnly": "npm run lint && npm run build",
    "serve": "node server.js",
    "release": "standard-version",
    "publish-public": "npm publish --access public"
  },
  "keywords": [
    "gis",
    "survey",
    "mapping",
    "geospatial",
    "3d",
    "rtk"
  ],
  "author": "Brandon Lewis",
  "license": "MIT",
  "homepage": "https://github.com/BrandonLewis/gis-survey.js",
  "peerDependencies": {
    "gnss.js": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "gnss.js": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/config-array": "^0.20.0",
    "@eslint/object-schema": "^2.1.6",
    "@npmcli/package-json": "^6.1.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "better-docs": "^2.7.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.26.0",
    "glob": "^10.4.1",
    "jsdoc": "^4.0.4",
    "jsdoc-to-markdown": "^9.1.1",
    "lru-cache": "^10.2.0",
    "read-package-up": "^11.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.40.2",
    "semantic-release": "^23.0.0",
    "standard-version": "^9.5.0"
  },
  "type": "module"
}
